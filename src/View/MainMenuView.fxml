        <font>
            <Font size="14.0" />
        </font></TextArea>
    <Button fx:id="listPartPageButton" layoutX="691.0" layoutY="29.0" mnemonicParsing="false" onAction="#goto_ListPartPage" prefHeight="30.0" prefWidth="80.0" text="List Parts" textAlignment="CENTER" wrapText="true" />
    <Button fx:id="definePartPageButton" layoutX="789.0" layoutY="29.0" mnemonicParsing="false" onAction="#goto_DefinePartPage" prefHeight="30.0" prefWidth="80.0" text="Define Part" />
    <Label layoutX="14.0" layoutY="14.0" opacity="0.7" prefHeight="30.0" prefWidth="189.0" text="Stock Management">
        <font>
            <Font name="System Bold" size="20.0" />
        </font>
    </Label>
   <Label layoutX="14.0" layoutY="97.0" prefHeight="30.0" prefWidth="40.0" text="Name">
