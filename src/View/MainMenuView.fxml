<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Control.MainMenuController">
    <ComboBox fx:id="mainMenuSearchComboBox" editable="true" layoutX="232.0" layoutY="87.0" onAction="#displaySearchedPart" prefHeight="25.0" prefWidth="308.0" promptText="Search Component" />
    <TextField fx:id="summaryNameTextF" editable="false" layoutX="14.0" layoutY="127.0" prefHeight="49.0" prefWidth="730.0" promptText="Component Name" />
    <TextField fx:id="summaryValueTextF" editable="false" layoutX="14.0" layoutY="200.0" prefHeight="317.0" prefWidth="730.0" promptText="Component Values" />
    <TextField fx:id="summaryCountTextF" editable="false" layoutX="577.0" layoutY="532.0" prefHeight="38.0" prefWidth="167.0" promptText="Component Count">
