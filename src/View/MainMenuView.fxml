<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Control.MainMenuController">
    <ComboBox fx:id="mainMenuSearchComboBox" editable="true" layoutX="232.0" layoutY="87.0" onAction="#searchFeatures" prefHeight="25.0" prefWidth="308.0" promptText="Search Part" />
    <TextField fx:id="summaryNameTextF" editable="false" layoutX="14.0" layoutY="127.0" prefHeight="38.0" prefWidth="730.0" promptText="Part Name" />
    <TextField fx:id="summaryCountTextF" alignment="CENTER" editable="false" layoutX="719.0" layoutY="551.0" prefHeight="35.0" prefWidth="45.0" promptText="0">
      <font>
         <Font name="System Bold" size="15.0" />
      </font></TextField>
    <TableView fx:id="displayFeaturesTableView" layoutX="14.0" layoutY="189.0" prefHeight="350.0" prefWidth="750.0">
     <columns>
       <TableColumn fx:id="displaySpecColumn" editable="false" prefWidth="365.0" text="Spec" />
       <TableColumn fx:id="displayValueColumn" editable="false" minWidth="0.0" prefWidth="360.0" text="Value" />
     </columns>
   </TableView>
    <Button fx:id="incBut" layoutX="790.0" layoutY="240.0" mnemonicParsing="false" onAction="#increaseCountPressed" prefHeight="35.0" prefWidth="65.0" text="Increase" textAlignment="CENTER" />
    <TextField fx:id="incTextField" alignment="CENTER" layoutX="800.0" layoutY="286.0" prefHeight="35.0" prefWidth="45.0" text="1">
        <font>
            <Font size="14.0" />
        </font></TextField>
    <Button fx:id="decBut" layoutX="789.0" layoutY="385.0" mnemonicParsing="false" onAction="#decreaseCountPressed" prefHeight="35.0" prefWidth="65.0" text="Decrease" />
    <TextField fx:id="decTextField" alignment="CENTER" layoutX="799.0" layoutY="429.0" prefHeight="35.0" prefWidth="45.0" text="1">
        <font>
            <Font size="14.0" />
        </font></TextField>
    <Button fx:id="listPartPageButton" layoutX="691.0" layoutY="29.0" mnemonicParsing="false" onAction="#goto_ListPartPage" prefHeight="30.0" prefWidth="80.0" text="List Parts" textAlignment="CENTER" underline="true" wrapText="true" />
    <Button fx:id="definePartPageButton" layoutX="789.0" layoutY="29.0" mnemonicParsing="false" onAction="#goto_DefinePartPage" prefHeight="30.0" prefWidth="80.0" text="Define Part" underline="true" />
    <Label layoutX="14.0" layoutY="14.0" opacity="0.7" prefHeight="30.0" prefWidth="189.0" text="Stock Management">
        <font>
            <Font name="System Bold" size="20.0" />
        </font>
    </Label>
   <Label layoutX="14.0" layoutY="97.0" prefHeight="30.0" prefWidth="40.0" text="Name">
      <font>
         <Font name="System Bold Italic" size="14.0" />
      </font>
   </Label>
   <Label layoutX="14.0" layoutY="165.0" prefHeight="30.0" prefWidth="57.0" text="Features" textAlignment="CENTER">
      <font>
         <Font name="System Bold Italic" size="14.0" />
      </font>
   </Label>
   <Label layoutX="669.0" layoutY="554.0" prefHeight="30.0" prefWidth="40.0" text="Count">
      <font>
         <Font name="System Bold Italic" size="14.0" />
      </font>
   </Label>
</AnchorPane>
