            <Font size="13.0" />
        </font></TextField>
    <Button fx:id="incBut" layoutX="781.0" layoutY="226.0" mnemonicParsing="false" onAction="#increaseCountPressed" prefHeight="35.0" prefWidth="80.0" text="Increase" />
