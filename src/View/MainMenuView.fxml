        <font>
            <Font size="14.0" />
        </font></TextArea>
    <Button fx:id="decBut" layoutX="780.0" layoutY="371.0" mnemonicParsing="false" onAction="#decreaseCountPressed" prefHeight="35.0" prefWidth="80.0" text="Decrease" />
